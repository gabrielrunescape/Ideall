<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ideall.gabrielrunescape.com.br">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.EditActivity"
            android:label="@string/new_project"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".view.ContactActivity"
            android:label="@string/new_contact"
            android:parentActivityName=".view.ShowAllActivity" />
        <activity
            android:name=".view.ShowAllActivity"
            android:label="@string/new_contact"
            android:parentActivityName=".view.EditActivity"/>
    </application>

</manifest>